import numpy as np
a = np.array([1, 2, 3])
b = np.array([4,5,6])
c = np.array([7,8,9])
print a
print b
print c
d = []
d.append(a)
print d
d.append(b)
print d
d.append(c)
print d
print type(d)
e = np.vstack(d)
print e
print type(e)

[1 2 3]
[4 5 6]
[7 8 9]
[array([1, 2, 3])]
[array([1, 2, 3]), array([4, 5, 6])]
[array([1, 2, 3]), array([4, 5, 6]), array([7, 8, 9])]
<type 'list'>
[[1 2 3]
